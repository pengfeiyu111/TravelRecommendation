{"ast":null,"code":"import { editTeacher } from \"./api.js\";\nimport uploadPicInput from \"@/views/template/upload-pic-input\";\nimport uploadFileInput from \"@/views/template/upload-file-input\";\nimport dict from \"@/views/template/dict\";\nexport default {\n  name: \"edit\",\n  components: {\n    uploadPicInput,\n    uploadFileInput,\n    dict\n  },\n  props: {\n    data: Object\n  },\n  data() {\n    return {\n      submitLoading: false,\n      // 表单提交状态\n      form: {\n        // 添加或编辑表单对象初始化数据\n        name: \"\",\n        education: \"\",\n        age: 0,\n        graduated: \"\",\n        wages: 0,\n        status: \"\",\n        autograph: \"\",\n        resume: \"\",\n        remark: \"\"\n      },\n      // 表单验证规则\n      formValidate: {}\n    };\n  },\n  methods: {\n    init() {\n      this.handleReset();\n      this.form = this.data;\n    },\n    handleReset() {\n      this.$refs.form.resetFields();\n    },\n    handleSubmit() {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          editTeacher(this.form).then(res => {\n            this.submitLoading = false;\n            if (res.success) {\n              this.$Message.success(\"操作成功\");\n              this.submited();\n            }\n          });\n        }\n      });\n    },\n    close() {\n      this.$emit(\"close\", true);\n    },\n    submited() {\n      this.$emit(\"submited\", true);\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};","map":{"version":3,"names":["editTeacher","uploadPicInput","uploadFileInput","dict","name","components","props","data","Object","submitLoading","form","education","age","graduated","wages","status","autograph","resume","remark","formValidate","methods","init","handleReset","$refs","resetFields","handleSubmit","validate","valid","then","res","success","$Message","submited","close","$emit","mounted"],"sources":["src/views/demo/demo2/edit.vue"],"sourcesContent":["<template>\r\n<div>\r\n    <Card>\r\n        <div slot=\"title\">\r\n            <div class=\"edit-head\">\r\n                <a @click=\"close\" class=\"back-title\">\r\n                    <Icon type=\"ios-arrow-back\" />返回\r\n                </a>\r\n                <div class=\"head-name\">编辑</div>\r\n                <span></span>\r\n                <a @click=\"close\" class=\"window-close\">\r\n                    <Icon type=\"ios-close\" size=\"31\" class=\"ivu-icon-ios-close\" />\r\n                </a>\r\n            </div>\r\n        </div>\r\n        <Form ref=\"form\" :model=\"form\" :label-width=\"100\" :rules=\"formValidate\" label-position=\"left\">\r\n            <FormItem label=\"姓名\" prop=\"name\">\r\n                <Input v-model=\"form.name\" clearable style=\"width:570px\" />\r\n            </FormItem>\r\n            <FormItem label=\"学历\" prop=\"education\">\r\n                <dict dict=\"education\" v-model=\"form.education\" transfer clearable placeholder=\"请选择学历...\" style=\"width: 570px\" />\r\n            </FormItem>\r\n            <FormItem label=\"年龄\" prop=\"age\">\r\n                <InputNumber v-model=\"form.age\" min=\"0\" max=\"5000000\" style=\"width:570px\"></InputNumber>\r\n            </FormItem>\r\n            <FormItem label=\"毕业院校\" prop=\"graduated\">\r\n                <Input v-model=\"form.graduated\" clearable style=\"width:570px\" />\r\n            </FormItem>\r\n            <FormItem label=\"工资\" prop=\"wages\">\r\n                <InputNumber v-model=\"form.wages\" min=\"0\" max=\"5000000\" style=\"width:570px\"></InputNumber>\r\n            </FormItem>\r\n            <FormItem label=\"在职状态\" prop=\"status\">\r\n                <dict dict=\"occupationStatus\" v-model=\"form.status\" transfer clearable placeholder=\"请选择在职状态...\" style=\"width: 570px\" />\r\n            </FormItem>\r\n            <FormItem label=\"签名\" prop=\"autograph\">\r\n                <upload-pic-input v-model=\"form.autograph\" style=\"width:570px\"></upload-pic-input>\r\n            </FormItem>\r\n            <FormItem label=\"个人简历\" prop=\"resume\">\r\n                <upload-file-input v-model=\"form.resume\" style=\"width:570px\"></upload-file-input>\r\n            </FormItem>\r\n            <FormItem label=\"备注\" prop=\"remark\">\r\n                <Input v-model=\"form.remark\" clearable style=\"width:570px\" />\r\n            </FormItem>\r\n            <Form-item class=\"br\">\r\n                <Button @click=\"handleSubmit\" :loading=\"submitLoading\" type=\"primary\">提交并保存</Button>\r\n                <Button @click=\"handleReset\">重置</Button>\r\n                <Button type=\"dashed\" @click=\"close\">关闭</Button>\r\n            </Form-item>\r\n        </Form>\r\n    </Card>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n    editTeacher\r\n} from \"./api.js\";\r\nimport uploadPicInput from \"@/views/template/upload-pic-input\";\r\nimport uploadFileInput from \"@/views/template/upload-file-input\";\r\nimport dict from \"@/views/template/dict\";\r\nexport default {\r\n    name: \"edit\",\r\n    components: {\r\n        uploadPicInput,\r\n        uploadFileInput,\r\n        dict\r\n    },\r\n    props: {\r\n        data: Object\r\n    },\r\n    data() {\r\n        return {\r\n            submitLoading: false, // 表单提交状态\r\n            form: { // 添加或编辑表单对象初始化数据\r\n                name: \"\",\r\n                education: \"\",\r\n                age: 0,\r\n                graduated: \"\",\r\n                wages: 0,\r\n                status: \"\",\r\n                autograph: \"\",\r\n                resume: \"\",\r\n                remark: \"\",\r\n            },\r\n            // 表单验证规则\r\n            formValidate: {}\r\n        };\r\n    },\r\n    methods: {\r\n        init() {\r\n            this.handleReset();\r\n            this.form = this.data;\r\n        },\r\n        handleReset() {\r\n            this.$refs.form.resetFields();\r\n        },\r\n        handleSubmit() {\r\n            this.$refs.form.validate(valid => {\r\n                if (valid) {\r\n                    editTeacher(this.form).then(res => {\r\n                        this.submitLoading = false;\r\n                        if (res.success) {\r\n                            this.$Message.success(\"操作成功\");\r\n                            this.submited();\r\n                        }\r\n                    });\r\n                }\r\n            });\r\n        },\r\n        close() {\r\n            this.$emit(\"close\", true);\r\n        },\r\n        submited() {\r\n            this.$emit(\"submited\", true);\r\n        }\r\n    },\r\n    mounted() {\r\n        this.init();\r\n    }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\n// 建议引入通用样式 具体路径自行修改 可删除下面样式代码\r\n// @import \"../../../styles/single-common.less\";\r\n.edit-head {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    position: relative;\r\n\r\n    .back-title {\r\n        color: #515a6e;\r\n        display: flex;\r\n        align-items: center;\r\n    }\r\n\r\n    .head-name {\r\n        display: inline-block;\r\n        height: 20px;\r\n        line-height: 20px;\r\n        font-size: 16px;\r\n        color: #17233d;\r\n        font-weight: 500;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        white-space: nowrap;\r\n    }\r\n\r\n    .window-close {\r\n        z-index: 1;\r\n        font-size: 12px;\r\n        position: absolute;\r\n        right: 0px;\r\n        top: -5px;\r\n        overflow: hidden;\r\n        cursor: pointer;\r\n\r\n        .ivu-icon-ios-close {\r\n            color: #999;\r\n            transition: color .2s ease;\r\n        }\r\n    }\r\n\r\n    .window-close .ivu-icon-ios-close:hover {\r\n        color: #444;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAsDA,SACAA,WAAA,QACA;AACA,OAAAC,cAAA;AACA,OAAAC,eAAA;AACA,OAAAC,IAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAJ,cAAA;IACAC,eAAA;IACAC;EACA;EACAG,KAAA;IACAC,IAAA,EAAAC;EACA;EACAD,KAAA;IACA;MACAE,aAAA;MAAA;MACAC,IAAA;QAAA;QACAN,IAAA;QACAO,SAAA;QACAC,GAAA;QACAC,